@model IPackageVersionModel
@{
    ViewBag.Tab = "Packages";
}

<h1 class="page-heading">Undo Pending Edits for @Model.Id @Model.Version</h1>
<p class="message">
    Click 'Undo' to revert all pending metadata edits for package <b>@Model.Id @Model.Version</b>
</p>
<p>
    Note: pending edits can be applied before your request to undo them is received. Applied edits cannot be undone. Instead, you can edit the package again.
</p>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <input type="submit" value="Undo" title="UndoPending Edits" />
}